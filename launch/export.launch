<launch>

	<arg name="bag" default="$(find jaco_kinect_calib)/bags/test_with_colors_1.bag"/>

  	<node pkg="rosbag" type="play" name="rosbag" args="-d 2 $(arg bag)"/>
  	
  	<node name="extract" pkg="image_view" type="extract_images" respawn="false" output="screen" cwd="ROS_HOME">
    <remap from="image" to="/camera/rgb/image_color"/>
  </node>
</launch>